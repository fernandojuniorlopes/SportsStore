@model IEnumerable<Product>

@{
    ViewData["Title"] = "Index";
}

<h1>Product list</h1>

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.Name</td>
                <td>@p.Price</td>
            </tr>
        }
    </tbody>
</table>